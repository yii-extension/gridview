{
    "name": "yii-extension/gridview",
    "type": "library",
    "description": "GridView for Yii3",
    "keywords": [
        "yii",
        "gridview"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-json": "*",
        "php": "^7.4|^8.0",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/arrays": "^1.0",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/html": "^3.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/strings": "^2.0",
        "yiisoft/translator-formatter-intl": "^3.0@dev",
        "yiisoft/translator-message-php": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/widget": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.5",
        "yiisoft/aliases": "^1.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\GridView\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\GridView\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
